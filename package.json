{
  "name": "@bluelovers/temp-github-actions-example-npm",
  "version": "1.0.0",
  "description": "Find the Scrabble score for a given word",
  "author": "Steve Winton <stevewinton@gmail.com> (https://github.com/swinton)",
  "main": "index.js",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "start": "echo npm run start && node ./index.js",
    "test": "echo npm run test",
    "lint": "echo npm run lint"
  },
  "dependencies": {},
  "devDependencies": {

  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js",
      "lib/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
